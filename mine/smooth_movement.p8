pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
char = {
 x=0,
 y=0,
 speed=50
}

last_t=0

function _init()
end

function _draw()
 cls()
 map(0,0,-char.x,-char.y)
 -- spr(1,char.x,char.y)
 spr(1,64,64)
end

function _update60()
 ft=t()
 elapsed=ft-last_t
 last_t=ft
 max_skip_fps=15
 max_elapsed=1/max_skip_fps
 if elapsed>max_elapsed then
  -- make sure we don't skip
  -- physics too far if the
  -- game hiccups
  elapsed=max_elapsed
 end
 local x=0
 local y=0
 if btn(⬅️) then
  x=-1
 end
 if btn(➡️) then
  x=1
 end
 if btn(⬆️) then
  y=-1
 end
 if btn(⬇️) then
  y=1
 end
 if x!=0 or y!=0 then
	 -- normalize the movement vector
	 local len=sqrt(x^2+y^2)
	 x/=len
	 y/=len
	 x*=char.speed
	 y*=char.speed
	 x*=elapsed
	 y*=elapsed
	 char.x += x
	 char.y += y
	end
end
