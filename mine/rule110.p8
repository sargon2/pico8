version 42
__lua__

function _init()
	cls(7)
	pset(127,0,0) -- seed
	for y=1,127 do
		for x=0,127 do
			local left=pget(x-1,y-1)!=7
			local center=pget(x,y-1)!=7
			local right=pget(x+1,y-1)!=7
			
			local set=0
			local sets = {
				[110]=true,
				[101]=true,
				[ 11]=true,
				[ 10]=true,
				[  1]=true,
			}

			local val = 0
			if left then
				val += 100
			end
			if center then
				val += 10
			end
			if right then
				val += 1
			end
			-- print(tostr(val)..tostr(sets[val]),3)
			if sets[val] then
				pset(x,y,0)
			end
		end
	end
end
